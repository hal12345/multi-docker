{
  "AWSEBDockerrunVersion": "1",
  "containerDefinitions": [
    {
      "name" : "client",
      "image": "z1234z6789z/multi-client",
      "hostname": "client",
      "essential": false,
      "memory": 128
    },
    {
      "name": "server",
      "image": "z1234z6789z/multi-server",
      "hostname": "api",
      "essential": false,
      "memory": 128
    },
   {
     "name": "worker",
     "image": "z1234z6789z/multi-worker",
     "hostname": "worker",
     "essential": false,
     "memory": 128
   },
   {
     "name": "nginx",
     "image": "z1234z6789z/multi-nginx",
     "hostname": "nginx",
     "essential": true,
     "portMappings":[
       {
         "hostPorts": 80,
         "containerPort": 80
       }
     ],
     "links": ["client", "server"],
     "memory": 128
   }

  ]

}
